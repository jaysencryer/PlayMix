{
  "name": "playmix-react-app",
  "version": "1.0.1",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "eslint": "eslint \"src/**/*.js\"",
    "start": "concurrently \"npm run dev:server\" \"npm run dev:bundler\"",
    "dev:server": "cross-env NODE_PATH=./src nodemon --exec \"babel-node src/server/server.js\" --ignore .reactful.json --ignore public/",
    "dev:bundler": "webpack -w --mode=development",
    "build": "npm install && npm run build:react && npm run build:node",
    "build:react": "cross-env NODE_ENV=production webpack --progress --mode=production",
    "build:node": "babel src -d build --config-file ./babel-node.config.js --copy-files",
    "build:all": "npm install && npm run build:react && npm run build:node",
    "prod:start": "cross-env NODE_ENV=production NODE_PATH=./build pm2 start build/server/server.js --update-env --name playmix-react-appProd && pm2 logs all",
    "prod:stop": "pm2 stop playmix-react-appProd",
    "prod:reload": "pm2 reload --update-env playmix-react-appProd",
    "prod:logs": "pm2 logs --update-env playmix-react-appProd",
    "test": "jest",
    "verify-tests": "jest --coverage"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ]
  },
  "dependencies": {
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.21.4",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.8",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "material-ui": "^0.20.2",
    "mongoose": "^6.4.4",
    "morgan": "^1.10.0",
    "pm2": "^5.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.0",
    "regenerator-runtime": "^0.13.7",
    "serialize-javascript": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.13.10",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "concurrently": "^6.0.0",
    "css-loader": "^5.2.7",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.2",
    "nodemon": "^2.0.7",
    "reactful": "^3.2.4",
    "style-loader": "^2.0.0",
    "webpack": "^5.30.0",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^4.9.2"
  }
}
